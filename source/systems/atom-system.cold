SETATOMMULTITHREADING(){
	uint8 code;
	if(!atomSystem){
		ret false;
	}
	multithreaded = !!multithreaded;
	if(multithreaded == (*atomSystem).multithreaded){
		code = NOTHING_DONE_ATOM_CODE;
		jmp emergencyExit;
	}
	if(multithreaded){
		if(!createLock(&(*atomSystem).lockSystem)){
			code = FAILED_MUTEX_ATOM_CODE;
			jmp emergencyExit;
		}
	}else{
		if(!destroyLock(&(*atomSystem).lockSystem)){
			code = FAILED_MUTEX_ATOM_CODE;
			jmp emergencyExit;
		}
	}
	setLockMultithreading(&(*atomSystem).lockSystem, multithreaded);
	(*atomSystem).multithreaded = multithreaded;
	code = SUCCESS_ATOM_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*atomSystem).operation = SET_ATOM_MULTITHREADING_ATOM_OPERATION;
		(*atomSystem).code = code;
		if(code != SUCCESS_ATOM_CODE){
			ret false;
		}
		ret true;
	}
}
