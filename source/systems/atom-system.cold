PRINTATOMLOG(){
	const char *operation;
	const char *code;
	PrintSystem *printSystem;
	bool success;
	if(!atomSystem){
		ret false;
	}
	if(!lockWriteLock(&(*atomSystem).lockSystem)){
		ret false;
	}
	printSystem = (*atomSystem).printSystem;
	success = false;
	if(!printSystem){
		jmp emergencyExit;
	}
	if(!lockMutex(&(*printSystem).mutexSystem)){
		jmp emergencyExit;
	}
	switch((*atomSystem).operation){
		to NONE_ATOM_OPERATION: /*------------------------------*/ operation = NULL; /*----------------------------*/ brk;
		to SET_ATOM_MEMORY_MANAGEMENT_SYSTEM_ATOM_OPERATION: /*-*/ operation = "setAtomMemoryManagementSystem"; /*-*/ brk;
		to SET_ATOM_PRINT_SYSTEM_ATOM_OPERATION: /*-------------*/ operation = "setAtomPrintSystem"; /*------------*/ brk;
		to SET_ATOM_CONNECTION_SYSTEM_ATOM_OPERATION: /*--------*/ operation = "setAtomConnectionSystem"; /*-------*/ brk;
		to SET_ATOM_ATOM_OPERATION: /*--------------------------*/ operation = "setAtom"; /*-----------------------*/ brk;
		to SET_ATOM_MULTITHREADING_ATOM_OPERATION: /*-----------*/ operation = "setAtomMultithreading"; /*---------*/ brk;
		to REQUEST_ATOMS_ATOM_OPERATION: /*---------------------*/ operation = "requestAtoms"; /*------------------*/ brk;
		to GET_ATOM_ATOM_OPERATION: /*--------------------------*/ operation = "getAtom"; /*-----------------------*/ brk;
		off: /*-------------------------------------------------*/ operation = "<operation>"; /*-------------------*/ brk;
	}
	switch((*atomSystem).code){
		to NONE_ATOM_CODE: /*-----------------------------------*/ code = NULL; /*---------------------------------*/ brk;
		to SUCCESS_ATOM_CODE: /*--------------------------------*/ code = "success"; /*----------------------------*/ brk;
		to NOTHING_DONE_ATOM_CODE: /*---------------------------*/ code = "nothing done"; /*-----------------------*/ brk;
		to BAD_INPUT_ATOM_CODE: /*------------------------------*/ code = "bad input"; /*--------------------------*/ brk;
		to UNSET_VALUE_ATOM_CODE: /*----------------------------*/ code = "unset value"; /*------------------------*/ brk;
		to CLOSED_CONNECTION_ATOM_CODE: /*----------------------*/ code = "closed connection"; /*------------------*/ brk;
		to FAILED_MUTEX_ATOM_CODE: /*---------------------------*/ code = "failed mutex"; /*-----------------------*/ brk;
		off: /*-------------------------------------------------*/ code = "<code>"; /*-----------------------------*/ brk;
	}
	if(operation and code){
		if((*atomSystem).code == SUCCESS_ATOM_CODE){
			if(printUnlockedOutput(printSystem, "%s%s: [%ssuccess%s%s] atom system:%s %s() reported \"%s\"\n", (*printSystem).outputColorStart, (*printSystem).programName, (*printSystem).outputColorEvent, (*printSystem).outputColorEnd, (*printSystem).outputColorStart, (*printSystem).outputColorEnd, operation, code)){
				success = true;
			}
		}else{
			if(printUnlockedError(printSystem, "%s%s: [%sfailure%s%s] atom system:%s %s() reported \"%s\"\n", (*printSystem).errorColorStart, (*printSystem).programName, (*printSystem).errorColorError, (*printSystem).errorColorEnd, (*printSystem).errorColorStart, (*printSystem).errorColorEnd, operation, code)){
				success = true;
			}
		}
	}
	unlockMutex(&(*printSystem).mutexSystem);
	jmp emergencyExit;
	emergencyExit:{
		unlockLock(&(*atomSystem).lockSystem);
		if(!success){
			ret false;
		}
		ret true;
	}
}

