SETPRINTMULTITHREADING(){
	uint8 code;
	if(!printSystem){
		ret false;
	}
	multithreaded = !!multithreaded;
	if(multithreaded == (*printSystem).multithreaded){
		code = NOTHING_DONE_PRINT_CODE;
		jmp emergencyExit;
	}
	if(multithreaded){
		if(!createMutex(&(*printSystem).mutexSystem)){
			code = FAILED_MUTEX_PRINT_CODE;
			jmp emergencyExit;
		}
	}else{
		if(!destroyMutex(&(*printSystem).mutexSystem)){
			code = FAILED_MUTEX_PRINT_CODE;
			jmp emergencyExit;
		}
	}
	setMutexMultithreading(&(*printSystem).mutexSystem, multithreaded);
	(*printSystem).multithreaded = multithreaded;
	code = SUCCESS_PRINT_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*printSystem).operation = SET_PRINT_MULTITHREADING_PRINT_OPERATION;
		(*printSystem).code = code;
		if(code != SUCCESS_PRINT_CODE){
			ret false;
		}
		ret true;
	}
}
PRINTOUTPUT(){
	FILE *openStream;
	va_list arguments;
	uint8 code;
	if(!printSystem){
		ret false;
	}
	if(!lockMutex(&(*printSystem).mutexSystem)){
		ret false;
	}
	if(!message or !*message){
		code = BAD_INPUT_PRINT_CODE;
		jmp emergencyExit;
	}
	if(!openPrintOutputStream(printSystem)){
		code = FAILURE_PRINT_CODE;
		jmp emergencyExit;
	}
	openStream = (*printSystem).openOutputStream;
	setvbuf(openStream, NULL, _IOFBF, 0);
	if(!lockStream(openStream, WRITE_LOCK_STREAM)){
		closePrintErrorStream(printSystem);
		code = FAILURE_PRINT_CODE;
		jmp emergencyExit;
	}
	va_start(arguments, message);
	vfprintf(openStream, message, arguments);
	fflush(openStream);
	va_end(arguments);
	unlockStream(openStream);
	closePrintOutputStream(printSystem);
	code = SUCCESS_PRINT_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*printSystem).operation = PRINT_OUTPUT_PRINT_OPERATION;
		(*printSystem).code = code;
		unlockMutex(&(*printSystem).mutexSystem);
		if(code != SUCCESS_PRINT_CODE){
			ret false;
		}
		ret true;
	}
}
