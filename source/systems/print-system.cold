PRINTUNACCOUNTEDFORPOINTERS(){
	FILE *openStream;
	MemoryManagementPointer *current;
	MemoryManagementPointer *wall;
	uint8 code;
	if(!printSystem){
		ret false;
	}
	if(!lockMutex(&(*printSystem).mutexSystem)){
		ret false;
	}
	if(!memoryManagementSystem){
		code = BAD_INPUT_PRINT_CODE;
		jmp emergencyExit;
	}
	if(!lockMutex(&(*memoryManagementSystem).mutexSystem)){
		code = FAILED_MUTEX_PRINT_CODE;
		jmp emergencyExit;
	}
	if((*memoryManagementSystem).pointerAllocated < 2){
		unlockMutex(&(*memoryManagementSystem).mutexSystem);
		code = NOTHING_DONE_PRINT_CODE;
		jmp emergencyExit;
	}
	if(!openPrintErrorStream(printSystem)){
		unlockMutex(&(*memoryManagementSystem).mutexSystem);
		code = FAILURE_PRINT_CODE;
		jmp emergencyExit;
	}
	openStream = (*printSystem).openErrorStream;
	setvbuf(openStream, NULL, _IOFBF, 0);
	if(!lockStream(openStream, WRITE_LOCK_STREAM)){
		closePrintErrorStream(printSystem);
		unlockMutex(&(*memoryManagementSystem).mutexSystem);
		code = FAILURE_PRINT_CODE;
		jmp emergencyExit;
	}
	current = (*memoryManagementSystem).pointerStart + 1;
	wall = current + (*memoryManagementSystem).pointerAllocated;
	code = NOTHING_DONE_PRINT_CODE;
	jmp printUnaccountedForPointerLoop;
	printUnaccountedForPointerLoop:{
		if(current < wall){
			if((*current).dataToken){
				const char *label = (*current).dataLabel;
				if(!label){
					label = "";
				}
				if(code != SUCCESS_PRINT_CODE){
					fprintf(openStream, "%s%s: %s%s:%s\n", (*printSystem).errorColorStart, (*printSystem).programName, (*printSystem).errorColorError, "unaccounted pointers", (*printSystem).errorColorEnd);
				}
				fprintf(openStream, "\t%s: line %u: %s\n", (*current).file, (*current).line, label);
				code = SUCCESS_PRINT_CODE;
			}
			inc current;
			jmp printUnaccountedForPointerLoop;
		}
		if(code == SUCCESS_PRINT_CODE){
			fflush(openStream);
		}
		unlockStream(openStream);
		closePrintErrorStream(printSystem);
		unlockMutex(&(*memoryManagementSystem).mutexSystem);
		jmp emergencyExit;
	}
	emergencyExit:{
		(*printSystem).operation = PRINT_UNACCOUNTED_FOR_POINTERS_PRINT_OPERATION;
		(*printSystem).code = code;
		unlockMutex(&(*printSystem).mutexSystem);
		if(code != SUCCESS_PRINT_CODE){
			ret false;
		}
		ret true;
	}
}
