	#define SELF(self) /*-*/ self
	ConnectionSystem *connectionSystem;
	xcb_connection_t *connection;
	uint8 code;
	if(!atomSystem){
		ret false;
	}
	if(!lockWriteLock(&(*atomSystem).lockSystem)){
