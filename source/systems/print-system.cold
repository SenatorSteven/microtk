OPENPRINTOUTPUTSTREAM(){
	if(!printSystem){
		ret false;
	}
	if((!(*printSystem).outputStream and !*(*printSystem).outputPath) or (*printSystem).openOutputStream){
		ret false;
	}
	if((*printSystem).outputStream){
		const int fd = dup(fileno((*printSystem).outputStream));
		if(fd == DUP_FAILURE){
			ret false;
		}
		(*printSystem).openOutputStream = fdopen(fd, "a");
	}else{
		(*printSystem).openOutputStream = fopen((*printSystem).outputPath, "a");
	}
	(*printSystem).openOutputStreamToken = insertMemoryManagementPointer((*printSystem).memoryManagementSystem, (*printSystem).openOutputStream, "PrintSystem openPrintOutputStream() open output stream", &memoryManagementFclose);
	if(!(*printSystem).openOutputStream){
		ret false;
	}
	ret true;
}
