INITPRINTSYSTEM(){
	if(!printSystem){
		ret false;
	}
	initMutexSystem(&(*printSystem).mutexSystem);
	(*printSystem).memoryManagementSystem = NULL;
	(*printSystem).programName = "";
	(*printSystem).outputStream = NULL;
	(*printSystem).outputPath = "";
	(*printSystem).errorStream = NULL;
	(*printSystem).errorPath = "";
	(*printSystem).outputColorStart = "";
	(*printSystem).outputColorEvent = "";
	(*printSystem).outputColorOutput = "";
	(*printSystem).outputColorError = "";
	(*printSystem).outputColorFatal = "";
	(*printSystem).outputColorEnd = "";
	(*printSystem).errorColorStart = "";
	(*printSystem).errorColorEvent = "";
	(*printSystem).errorColorOutput = "";
	(*printSystem).errorColorError = "";
	(*printSystem).errorColorFatal = "";
	(*printSystem).errorColorEnd = "";
	(*printSystem).openOutputStream = NULL;
	(*printSystem).openOutputStreamToken = NONE_MEMORY_MANAGEMENT_TOKEN;
	(*printSystem).openErrorStream = NULL;
	(*printSystem).openErrorStreamToken = NONE_MEMORY_MANAGEMENT_TOKEN;
	(*printSystem).operation = NONE_PRINT_OPERATION;
	(*printSystem).code = NONE_PRINT_CODE;
	(*printSystem).multithreaded = false;
	ret true;
}
SETPRINTMEMORYMANAGEMENTSYSTEM(){
	uint8 code;
	if(!printSystem){
		ret false;
	}
	if(!lockMutex(&(*printSystem).mutexSystem)){
		ret false;
	}
	if(memoryManagementSystem == (*printSystem).memoryManagementSystem){
		code = NOTHING_DONE_PRINT_CODE;
		jmp emergencyExit;
	}
	if(memoryManagementSystem){
		(*printSystem).openOutputStreamToken = insertMemoryManagementPointer(memoryManagementSystem, (*printSystem).openOutputStream, "PrintSystem setPrintMemoryManagementSystem() open output stream", &memoryManagementFclose);
		(*printSystem).openErrorStreamToken = insertMemoryManagementPointer(memoryManagementSystem, (*printSystem).openErrorStream, "PrintSystem setPrintMemoryManagementSystem() open error stream", &memoryManagementFclose);
	}else{
		removeMemoryManagementPointer((*printSystem).memoryManagementSystem, (*printSystem).openOutputStreamToken);
		removeMemoryManagementPointer((*printSystem).memoryManagementSystem, (*printSystem).openErrorStreamToken);
	}
	(*printSystem).memoryManagementSystem = memoryManagementSystem;
	code = SUCCESS_PRINT_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*printSystem).operation = SET_PRINT_MEMORY_MANAGEMENT_SYSTEM_PRINT_OPERATION;
		(*printSystem).code = code;
		unlockMutex(&(*printSystem).mutexSystem);
		if(code != SUCCESS_PRINT_CODE){
			ret false;
		}
		ret true;
	}
}
