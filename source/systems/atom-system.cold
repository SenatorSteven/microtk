GETATOM(){
	uint8 code;
	if(!atomSystem){
		ret false;
	}
	if(!lockReadLock(&(*atomSystem).lockSystem)){
		ret false;
	}
	if(!atom or atom > ST_Sn_ATOM){
		code = BAD_INPUT_ATOM_CODE;
		jmp emergencyExit;
	}
	*atomReturn = *(&(*atomSystem).ICCCM_NAME + atom - 1);
	code = SUCCESS_ATOM_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*atomSystem).operation = GET_ATOM_ATOM_OPERATION;
		(*atomSystem).code = code;
		unlockLock(&(*atomSystem).lockSystem);
		if(code != SUCCESS_ATOM_CODE){
			ret false;
		}
		ret true;
	}
}
FINIATOMSYSTEM(){
	if(!atomSystem){
		ret false;
	}
	if(!finiLockSystem(&(*atomSystem).lockSystem)){
		ret false;
	}
	if(!finiAvlTreeSystem(&(*atomSystem).avlTreeSystem)){
		ret false;
	}
