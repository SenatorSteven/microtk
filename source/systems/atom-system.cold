SETATOMMEMORYMANAGEMENTSYSTEM(){
	uint8 code;
	if(!atomSystem){
		ret false;
	}
	if(!lockWriteLock(&(*atomSystem).lockSystem)){
		ret false;
	}
	if(memoryManagementSystem == (*atomSystem).memoryManagementSystem){
		code = NOTHING_DONE_ATOM_CODE;
		jmp emergencyExit;
	}
	setAvlTreeMemoryManagementSystem(&(*atomSystem).avlTreeSystem, memoryManagementSystem);
	(*atomSystem).memoryManagementSystem = memoryManagementSystem;
	code = SUCCESS_ATOM_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*atomSystem).operation = SET_ATOM_MEMORY_MANAGEMENT_SYSTEM_ATOM_OPERATION;
		(*atomSystem).code = code;
		unlockLock(&(*atomSystem).lockSystem);
		if(code != SUCCESS_ATOM_CODE){
			ret false;
		}
		ret true;
	}
}
