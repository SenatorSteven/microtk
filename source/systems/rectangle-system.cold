	(*rectangleSystem).yOffsetType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).xOffsetType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).yType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).xType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).selectedBackground = 0x00000000;
	(*rectangleSystem).selectedForeground = 0x00000000;
	(*rectangleSystem).background = 0x00000000;
	(*rectangleSystem).foreground = 0x00000000;
	(*rectangleSystem).cornerRadiusBottomRight = 0;
	(*rectangleSystem).cornerRadiusBottomLeft = 0;
	(*rectangleSystem).cornerRadiusTopRight = 0;
	(*rectangleSystem).cornerRadiusTopLeft = 0;
	(*rectangleSystem).verticalMargin = 0;
	(*rectangleSystem).horizontalMargin = 0;
	(*rectangleSystem).verticalPadding = 0;
	(*rectangleSystem).horizontalPadding = 0;
	(*rectangleSystem).height = 0;
	(*rectangleSystem).width = 0;
	(*rectangleSystem).yOffset = 0;
	(*rectangleSystem).xOffset = 0;
	(*rectangleSystem).y = 0;
	(*rectangleSystem).x = 0;
	(*rectangleSystem).printSystem = NULL;
	ret true;
}
PRINTRECTANGLELOG(){
	const char *operation;
	const char *code;
	PrintSystem *printSystem;
	bool success;
	if(!rectangleSystem){
		ret false;
	}
	if(!lockMutex(&(*rectangleSystem).mutexSystem)){
		ret false;
	}
	printSystem = (*rectangleSystem).printSystem;
	success = false;
	if(!printSystem){
		jmp emergencyExit;
	}
	if(!lockMutex(&(*printSystem).mutexSystem)){
		jmp emergencyExit;
	}
	switch((*rectangleSystem).operation){
		to NONE_RECTANGLE_OPERATION: /*------------------------------*/ operation = NULL; /*-----------------------------*/ brk;
		to SET_RECTANGLE_PRINT_SYSTEM_RECTANGLE_OPERATION: /*--------*/ operation = "setRectanglePrintSystem"; /*--------*/ brk;
		to SET_RECTANGLE_POSITION_RECTANGLE_OPERATION: /*------------*/ operation = "setRectanglePosition"; /*-----------*/ brk;
		to SET_RECTANGLE_OFFSET_RECTANGLE_OPERATION: /*--------------*/ operation = "setRectangleOffset"; /*-------------*/ brk;
		to SET_RECTANGLE_SIZE_RECTANGLE_OPERATION: /*----------------*/ operation = "setRectangleSize"; /*---------------*/ brk;
		to SET_RECTANGLE_PADDING_RECTANGLE_OPERATION: /*-------------*/ operation = "setRectanglePadding"; /*------------*/ brk;
		to SET_RECTANGLE_MARGIN_RECTANGLE_OPERATION: /*--------------*/ operation = "setRectangleMargin"; /*-------------*/ brk;
		to SET_RECTANGLE_PLACEMENT_RECTANGLE_OPERATION: /*-----------*/ operation = "setRectanglePlacement"; /*----------*/ brk;
		to SET_RECTANGLE_ALIGNMENT_RECTANGLE_OPERATION: /*-----------*/ operation = "setRectangleAlignment"; /*----------*/ brk;
		to SET_RECTANGLE_CLIPPING_RECTANGLE_OPERATION: /*------------*/ operation = "setRectangleClipping"; /*-----------*/ brk;
		to SET_RECTANGLE_CORNER_RADIUS_RECTANGLE_OPERATION: /*-------*/ operation = "setRectangleCornerRadius"; /*-------*/ brk;
		to SET_RECTANGLE_FOREGROUND_RECTANGLE_OPERATION: /*----------*/ operation = "setRectangleForeground"; /*---------*/ brk;
		to SET_RECTANGLE_BACKGROUND_RECTANGLE_OPERATION: /*----------*/ operation = "setRectangleBackground"; /*---------*/ brk;
		to SET_RECTANGLE_SELECTED_FOREGROUND_RECTANGLE_OPERATION: /*-*/ operation = "setRectangleSelectedForeground"; /*-*/ brk;
		to SET_RECTANGLE_SELECTED_BACKGROUND_RECTANGLE_OPERATION: /*-*/ operation = "setRectangleSelectedBackground"; /*-*/ brk;
		to SET_RECTANGLE_MULTITHREADING_RECTANGLE_OPERATION: /*------*/ operation = "setRectangleMultithreading"; /*-----*/ brk;
		to GET_RECTANGLE_RECTANGLE_RECTANGLE_OPERATION: /*-----------*/ operation = "getRectangleRectangle"; /*----------*/ brk;
		off: /*------------------------------------------------------*/ operation = "<operation>"; /*--------------------*/ brk;
	}
	switch((*rectangleSystem).code){
		to NONE_RECTANGLE_CODE: /*-----------------------------------*/ code = NULL; /*----------------------------------*/ brk;
		to SUCCESS_RECTANGLE_CODE: /*--------------------------------*/ code = "success"; /*-----------------------------*/ brk;
		to NOTHING_DONE_RECTANGLE_CODE: /*---------------------------*/ code = "nothing done"; /*------------------------*/ brk;
		to BAD_INPUT_RECTANGLE_CODE: /*------------------------------*/ code = "bad input"; /*---------------------------*/ brk;
		to FAILED_MUTEX_RECTANGLE_CODE: /*---------------------------*/ code = "failed mutex"; /*------------------------*/ brk;
		off: /*------------------------------------------------------*/ code = "<code>"; /*------------------------------*/ brk;
	}
	if(operation and code){
		if((*rectangleSystem).code == SUCCESS_RECTANGLE_CODE){
			if(printUnlockedOutput(printSystem, "%s%s: [%ssuccess%s%s] rectangle system:%s %s() reported \"%s\"\n", (*printSystem).outputColorStart, (*printSystem).programName, (*printSystem).outputColorEvent, (*printSystem).outputColorEnd, (*printSystem).outputColorStart, (*printSystem).outputColorEnd, operation, code)){
				success = true;
			}
		}else{
			if(printUnlockedError(printSystem, "%s%s: [%sfailure%s%s] rectangle system:%s %s() reported \"%s\"\n", (*printSystem).errorColorStart, (*printSystem).programName, (*printSystem).errorColorError, (*printSystem).errorColorEnd, (*printSystem).errorColorStart, (*printSystem).errorColorEnd, operation, code)){
				success = true;
			}
		}
	}
	unlockMutex(&(*printSystem).mutexSystem);
	jmp emergencyExit;
	emergencyExit:{
		unlockMutex(&(*rectangleSystem).mutexSystem);
		if(!success){
			ret false;
		}
		ret true;
	}
}

