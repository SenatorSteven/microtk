INITRECTANGLESYSTEM(){
	if(!rectangleSystem){
		ret false;
	}
	initMutexSystem(&(*rectangleSystem).mutexSystem);
	(*rectangleSystem).printSystem = NULL;
	(*rectangleSystem).x = 0;
	(*rectangleSystem).y = 0;
	(*rectangleSystem).xOffset = 0;
	(*rectangleSystem).yOffset = 0;
	(*rectangleSystem).width = 0;
	(*rectangleSystem).height = 0;
	(*rectangleSystem).horizontalPadding = 0;
	(*rectangleSystem).verticalPadding = 0;
	(*rectangleSystem).horizontalMargin = 0;
	(*rectangleSystem).verticalMargin = 0;
	(*rectangleSystem).cornerRadiusTopLeft = 0;
	(*rectangleSystem).cornerRadiusTopRight = 0;
	(*rectangleSystem).cornerRadiusBottomLeft = 0;
	(*rectangleSystem).cornerRadiusBottomRight = 0;
	(*rectangleSystem).foreground = 0x00000000;
	(*rectangleSystem).background = 0x00000000;
	(*rectangleSystem).selectedForeground = 0x00000000;
	(*rectangleSystem).selectedBackground = 0x00000000;
	(*rectangleSystem).xType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).yType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).xOffsetType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).yOffsetType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).widthType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).heightType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).horizontalPaddingType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).verticalPaddingType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).horizontalMarginType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).verticalMarginType = NONE_RECTANGLE_NUMBER_TYPE;
	(*rectangleSystem).placement = NONE_RECTANGLE_PLACEMENT;
	(*rectangleSystem).horizontalAlignment = NONE_RECTANGLE_HORIZONTAL_ALIGNMENT;
	(*rectangleSystem).verticalAlignment = NONE_RECTANGLE_VERTICAL_ALIGNMENT;
	(*rectangleSystem).clipChildren = false;
	(*rectangleSystem).operation = NONE_RECTANGLE_OPERATION;
	(*rectangleSystem).code = NONE_RECTANGLE_CODE;
	(*rectangleSystem).multithreaded = false;
	ret true;
}
SETRECTANGLEPRINTSYSTEM(){
	uint8 code;
	if(!rectangleSystem){
		ret false;
	}
	if(!lockMutex(&(*rectangleSystem).mutexSystem)){
		ret false;
	}
	if(printSystem == (*rectangleSystem).printSystem){
		code = NOTHING_DONE_RECTANGLE_CODE;
		jmp emergencyExit;
	}
	(*rectangleSystem).printSystem = printSystem;
	code = SUCCESS_RECTANGLE_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*rectangleSystem).operation = SET_RECTANGLE_PRINT_SYSTEM_RECTANGLE_OPERATION;
		(*rectangleSystem).code = code;
		unlockMutex(&(*rectangleSystem).mutexSystem);
		if(code != SUCCESS_RECTANGLE_CODE){
			ret false;
		}
		ret true;
	}
}
