	}
	if(!connectionIsOpen(connectionSystem)){
		unlockLock(&(*connectionSystem).lockSystem);
		code = CLOSED_CONNECTION_ATOM_CODE;
		jmp emergencyExit;
	}
	connection = (*connectionSystem).connection;
	jmp setAtoms;



























	setAtoms:{
		char wmString[lengthof("WM_S") + 3] = "WM_S";
		char cmString[lengthof("_NET_WM_CM_S") + 3] = "_NET_WM_CM_S";
		char stString[lengthof("_NET_SYSTEM_TRAY_S") + 3] = "_NET_SYSTEM_TRAY_S";
		char *atomNameStart[] = STRING(SELF);
		char *const *atomNameCurrent = atomNameStart;
		uint atomNameSizeStart[] = STRING(lengthof);
		uint *atomNameSizeCurrent = atomNameSizeStart;
		xcb_intern_atom_cookie_t cookieStart[countof(atomNameStart)];
		xcb_intern_atom_cookie_t *cookieCurrent = cookieStart;
		xcb_intern_atom_cookie_t *const cookieWall = cookieCurrent + countof(cookieStart);
		xcb_atom_t *atomCurrent = &(*atomSystem).ICCCM_NAME;
		jmp createCustomAtoms;
		createCustomAtoms:{
			const uint screenNumber = XDefaultScreen((*connectionSystem).display);
			const uint size = getIntegerByteSize(screenNumber);
			writeIntegerString(screenNumber, wmString + lengthof("WM_S"));
			writeIntegerString(screenNumber, cmString + lengthof("_NET_WM_CM_S"));
			writeIntegerString(screenNumber, stString + lengthof("_NET_SYSTEM_TRAY_S"));
			*(atomNameSizeStart + countof(atomNameSizeStart) - 3) = lengthof("WM_S") + size;
			*(atomNameSizeStart + countof(atomNameSizeStart) - 2) = lengthof("_NET_WM_CM_S") + size;
			*(atomNameSizeStart + countof(atomNameSizeStart) - 1) = lengthof("_NET_SYSTEM_TRAY_S") + size;
			*(atomNameStart + countof(atomNameStart) - 3) = wmString;
			*(atomNameStart + countof(atomNameStart) - 2) = cmString;
			*(atomNameStart + countof(atomNameStart) - 1) = stString;
			jmp atomRequestLoop;
		}
		atomRequestLoop:{
			if(cookieCurrent < cookieWall){
				*cookieCurrent = xcb_intern_atom_unchecked(connection, false, *atomNameSizeCurrent, *atomNameCurrent);
				inc atomNameSizeCurrent;
				inc atomNameCurrent;
				inc cookieCurrent;
				jmp atomRequestLoop;
			}
			cookieCurrent = cookieStart;
			jmp atomReplyLoop;
		}
		atomReplyLoop:{
			if(cookieCurrent < cookieWall){
				xcb_intern_atom_reply_t *const reply = xcb_wait_for_reply(connection, *cookieCurrent, NULL);
				uint32 token = insertMemoryManagementPointer((*atomSystem).memoryManagementSystem, reply, "AtomSystem requestAtoms() xcb_intern_atom() reply", &memoryManagementFree);
				*atomCurrent = XCB_NONE;
				if(reply){
					*atomCurrent = (*reply).atom;
					free(reply);
					removeMemoryManagementPointer((*atomSystem).memoryManagementSystem, token);
				}
				inc atomCurrent;
				inc cookieCurrent;
				jmp atomReplyLoop;
			}
			unlockLock(&(*connectionSystem).lockSystem);
			jmp createAvlTree;
		}
	}
	createAvlTree:{
		if((*atomSystem).createTree){
			AvlTreeSystem *const avlTreeSystem = &(*atomSystem).avlTreeSystem;
			xcb_atom_t *start = &(*atomSystem).ICCCM_NAME;
			xcb_atom_t *current = start;
			xcb_atom_t *const wall = &(*atomSystem).ST_Sn + 1;
			removeAllAvlTreeNodes(avlTreeSystem);
			setAvlTreeBlockSize(avlTreeSystem, 128);
			jmp insertAtomLoop;
			insertAtomLoop:{
				if(current < wall){
					insertAvlTreeNode(avlTreeSystem, *current, current - start);
					inc current;
					jmp insertAtomLoop;
				}
			}
		}



























		code = SUCCESS_ATOM_CODE;
		jmp emergencyExit;
	}
	emergencyExit:{
