SETRECTANGLEMULTITHREADING(){
	uint8 code;
	if(!rectangleSystem){
		ret false;
	}
	multithreaded = !!multithreaded;
	if(multithreaded == (*rectangleSystem).multithreaded){
		code = NOTHING_DONE_RECTANGLE_CODE;
		jmp emergencyExit;
	}
	if(multithreaded){
		if(!createMutex(&(*rectangleSystem).mutexSystem)){
			code = FAILED_MUTEX_RECTANGLE_CODE;
			jmp emergencyExit;
		}
	}else{
		if(!destroyMutex(&(*rectangleSystem).mutexSystem)){
			code = FAILED_MUTEX_RECTANGLE_CODE;
			jmp emergencyExit;
		}
	}
	setMutexMultithreading(&(*rectangleSystem).mutexSystem, multithreaded);
	(*rectangleSystem).multithreaded = multithreaded;
	code = SUCCESS_RECTANGLE_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*rectangleSystem).operation = SET_RECTANGLE_MULTITHREADING_RECTANGLE_OPERATION;
		(*rectangleSystem).code = code;
		if(code != SUCCESS_RECTANGLE_CODE){
			ret false;
		}
		ret true;
	}
}
GETRECTANGLERECTANGLE(){
	int x;
	int y;
	int xOffset;
	int yOffset;
	uint width;
	uint height;
	uint horizontalPadding;
	uint verticalPadding;
	uint horizontalMargin;
	uint verticalMargin;
	uint8 code;
	if(!rectangleSystem){
		ret false;
	}
	if(!lockMutex(&(*rectangleSystem).mutexSystem)){
		ret false;
	}
	if(!rectangleReturn){
		code = BAD_INPUT_RECTANGLE_CODE;
		jmp emergencyExit;
	}
	x = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).x, (*rectangleSystem).xType, HORIZONTAL_RECTANGLE_AXIS, parentRectangleWidth, rootRectanglePixelWidth, rootRectangleMillimeterWidth);
	y = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).y, (*rectangleSystem).yType, VERTICAL_RECTANGLE_AXIS, parentRectangleHeight, rootRectanglePixelHeight, rootRectangleMillimeterHeight);
	xOffset = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).xOffset, (*rectangleSystem).xOffsetType, HORIZONTAL_RECTANGLE_AXIS, parentRectangleWidth, rootRectanglePixelWidth, rootRectangleMillimeterWidth);
	yOffset = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).yOffset, (*rectangleSystem).yOffsetType, VERTICAL_RECTANGLE_AXIS, parentRectangleHeight, rootRectanglePixelHeight, rootRectangleMillimeterHeight);
	width = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).width, (*rectangleSystem).widthType, HORIZONTAL_RECTANGLE_AXIS, parentRectangleWidth, rootRectanglePixelWidth, rootRectangleMillimeterWidth);
	height = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).height, (*rectangleSystem).heightType, VERTICAL_RECTANGLE_AXIS, parentRectangleHeight, rootRectanglePixelHeight, rootRectangleMillimeterHeight);
	horizontalPadding = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).horizontalPadding, (*rectangleSystem).horizontalPaddingType, HORIZONTAL_RECTANGLE_AXIS, parentRectangleWidth, rootRectanglePixelWidth, rootRectangleMillimeterWidth);
	verticalPadding = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).verticalPadding, (*rectangleSystem).verticalPaddingType, VERTICAL_RECTANGLE_AXIS, parentRectangleHeight, rootRectanglePixelHeight, rootRectangleMillimeterHeight);
	horizontalMargin = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).horizontalMargin, (*rectangleSystem).horizontalMarginType, HORIZONTAL_RECTANGLE_AXIS, parentRectangleWidth, rootRectanglePixelWidth, rootRectangleMillimeterWidth);
	verticalMargin = getRectangleTypedNumber(rectangleSystem, (*rectangleSystem).verticalMargin, (*rectangleSystem).verticalMarginType, VERTICAL_RECTANGLE_AXIS, parentRectangleHeight, rootRectanglePixelHeight, rootRectangleMillimeterHeight);
	if((*rectangleSystem).placement >= LEFT_RECTANGLE_PLACEMENT){
		height = parentRectangleHeight;
	}elif((*rectangleSystem).placement >= TOP_RECTANGLE_PLACEMENT){
		width = parentRectangleWidth;
	}
	if((*rectangleSystem).placement == LEFT_RECTANGLE_PLACEMENT){
		x = 0 * (parentRectangleWidth - width) / 2;
	}elif((*rectangleSystem).placement == RIGHT_RECTANGLE_PLACEMENT){
		x = 2 * (parentRectangleWidth - width) / 2;
	}elif((*rectangleSystem).horizontalAlignment){
		x = ((*rectangleSystem).horizontalAlignment - 1) * (parentRectangleWidth - width) / 2;
	}
	if((*rectangleSystem).placement == TOP_RECTANGLE_PLACEMENT){
		y = 0 * (parentRectangleHeight - height) / 2;
	}elif((*rectangleSystem).placement == BOTTOM_RECTANGLE_PLACEMENT){
		y = 2 * (parentRectangleHeight - height) / 2;
	}elif((*rectangleSystem).verticalAlignment){
		y = ((*rectangleSystem).verticalAlignment - 1) * (parentRectangleHeight - height) / 2;
	}
	(*rectangleReturn).rootRectanglePixelWidth = rootRectanglePixelWidth;
	(*rectangleReturn).rootRectanglePixelHeight = rootRectanglePixelHeight;
	(*rectangleReturn).rootRectangleMillimeterWidth = rootRectangleMillimeterWidth;
	(*rectangleReturn).rootRectangleMillimeterHeight = rootRectangleMillimeterHeight;
	(*rectangleReturn).x = x + xOffset;
	(*rectangleReturn).y = y + yOffset;
	(*rectangleReturn).width = width;
	(*rectangleReturn).height = height;
	(*rectangleReturn).horizontalPadding = horizontalPadding;
	(*rectangleReturn).verticalPadding = verticalPadding;
	(*rectangleReturn).horizontalMargin = horizontalMargin;
	(*rectangleReturn).verticalMargin = verticalMargin;
	(*rectangleReturn).cornerRadiusTopLeft = (*rectangleSystem).cornerRadiusTopLeft;
	(*rectangleReturn).cornerRadiusTopRight = (*rectangleSystem).cornerRadiusTopRight;
	(*rectangleReturn).cornerRadiusBottomLeft = (*rectangleSystem).cornerRadiusBottomLeft;
	(*rectangleReturn).cornerRadiusBottomRight = (*rectangleSystem).cornerRadiusBottomRight;
	(*rectangleReturn).foreground = (*rectangleSystem).foreground;
	(*rectangleReturn).background = (*rectangleSystem).background;
	(*rectangleReturn).selectedForeground = (*rectangleSystem).selectedForeground;
	(*rectangleReturn).selectedBackground = (*rectangleSystem).selectedBackground;
	(*rectangleReturn).placement = (*rectangleSystem).placement;
	(*rectangleReturn).horizontalAlignment = (*rectangleSystem).horizontalAlignment;
	(*rectangleReturn).verticalAlignment = (*rectangleSystem).verticalAlignment;
	(*rectangleReturn).clipChildren = (*rectangleSystem).clipChildren;
	code = SUCCESS_RECTANGLE_CODE;
	jmp emergencyExit;
	emergencyExit:{
		(*rectangleSystem).operation = GET_RECTANGLE_RECTANGLE_RECTANGLE_OPERATION;
		(*rectangleSystem).code = code;
		if(code != SUCCESS_RECTANGLE_CODE){
			ret false;
		}
		ret true;
	}
}
GETRECTANGLETYPEDNUMBER(){
	if(!rectangleSystem){
		ret 0;
	}
	switch(type){
		to NONE_RECTANGLE_NUMBER_TYPE: /*-------*/ ret 0;
		to INTEGER_RECTANGLE_NUMBER_TYPE: /*----*/ ret number;
		to PERCENTAGE_RECTANGLE_NUMBER_TYPE: /*-*/ ret number * parentSize / 100;
		to MILLIMETER_RECTANGLE_NUMBER_TYPE: /*-*/ brk;
		off: /*---------------------------------*/ ret 0;
	}
	if(!axis or axis > VERTICAL_RECTANGLE_AXIS or !rootRectangleMillimeterSize){
		ret 0;
	}
	ret number * rootRectanglePixelSize / rootRectangleMillimeterSize;
}
